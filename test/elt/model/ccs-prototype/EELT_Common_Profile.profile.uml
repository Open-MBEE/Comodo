  <DSLCustomization:derivedPropertySpecification xmi:id="_8-EVX3lfEeqvoY0JcwSHBQ" base_Property="5490e412-3eb2-4319-aa72-2051682192f5">
    <expression>&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; standalone=&quot;yes&quot;?>
&lt;callExpressionSpecification xmlns=&quot;http://www.nomagic.com/schemas/MagicDraw/StructuredExpression/2013&quot;>
    &lt;taggedValues>
        &lt;entry key=&quot;name&quot;>
            &lt;value>getTriggerSignalScope&lt;/value>
        &lt;/entry>
    &lt;/taggedValues>
    &lt;argument xsi:type=&quot;lookupExpressionSpecification&quot; symbol=&quot;THIS&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;/>
    &lt;argument xsi:type=&quot;callExpressionSpecification&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;>
        &lt;taggedValues>
            &lt;entry key=&quot;name&quot;>
                &lt;value>getSignals&lt;/value>
            &lt;/entry>
        &lt;/taggedValues>
        &lt;argument xsi:type=&quot;lookupExpressionSpecification&quot; symbol=&quot;THIS&quot;/>
        &lt;expression xsi:type=&quot;metaChainExpressionSpecification&quot;>
            &lt;chain xsi:type=&quot;umlProperty&quot; metaclass=&quot;Transition&quot; property=&quot;trigger&quot;/>
            &lt;chain xsi:type=&quot;umlProperty&quot; metaclass=&quot;Trigger&quot; property=&quot;event&quot;/>
            &lt;chain xsi:type=&quot;umlProperty&quot; metaclass=&quot;SignalEvent&quot; property=&quot;signal&quot;/>
        &lt;/expression>
    &lt;/argument>
    &lt;expression xsi:type=&quot;makeInlineExpressionSpecification&quot; language=&quot;Jython&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;>
        &lt;parameter name=&quot;arg1&quot; type=&quot;java.lang.Object&quot; ordered=&quot;false&quot; unique=&quot;false&quot; multiple=&quot;false&quot;/>
        &lt;parameter name=&quot;signals&quot; type=&quot;java.lang.Object&quot; ordered=&quot;false&quot; unique=&quot;false&quot; multiple=&quot;false&quot;/>
        &lt;body>#
# This script gets a signal of Signals and tries to build
# for each of them the scope as a hierachy of owning &amp;lt;&amp;lt;Interface Block&amp;gt;&amp;gt;s,
# assuming they are structured in this way.
#  
from com.nomagic.uml2.ext.jmi.helpers import StereotypesHelper
from com.nomagic.magicdraw.core import Application

from com.nomagic.uml2.ext.magicdraw.classes.mdkernel import TypedElement
from com.nomagic.uml2.ext.magicdraw.classes.mdkernel import NamedElement

res = &quot;&quot;
sep = &quot;&quot;

ifblockStereotype = StereotypesHelper.getStereotype(project, &quot;InterfaceBlock&quot;);   

signalsIter = signals.iterator()
while signalsIter.hasNext():
   signal = signalsIter.next()  
   if isinstance(signal,NamedElement):  
      owner = signal
      while True:
         owner = owner.getOwner()
         if owner is None:
            break
         if StereotypesHelper.hasStereotype(owner, ifblockStereotype):     
            res = owner.getName() + sep + res
            sep = &quot;.&quot;
         
result.set(res)&lt;/body>
    &lt;/expression>
&lt;/callExpressionSpecification>
</expression>
  </DSLCustomization:derivedPropertySpecification>
  <DSLCustomization:derivedPropertySpecification xmi:id="_8-EVYHlfEeqvoY0JcwSHBQ" base_Property="3d0fd7ec-28bf-4043-ab0d-7a39ce2235e0">
    <expression>&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; standalone=&quot;yes&quot;?>
&lt;callExpressionSpecification xmlns=&quot;http://www.nomagic.com/schemas/MagicDraw/StructuredExpression/2013&quot;>
    &lt;taggedValues>
        &lt;entry key=&quot;name&quot;>
            &lt;value>value&lt;/value>
        &lt;/entry>
    &lt;/taggedValues>
    &lt;argument xsi:type=&quot;lookupExpressionSpecification&quot; symbol=&quot;THIS&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;/>
    &lt;argument xsi:type=&quot;callExpressionSpecification&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;>
        &lt;taggedValues>
            &lt;entry key=&quot;name&quot;>
                &lt;value>getStereotypes&lt;/value>
            &lt;/entry>
        &lt;/taggedValues>
        &lt;argument xsi:type=&quot;lookupExpressionSpecification&quot; symbol=&quot;THIS&quot;/>
        &lt;expression xsi:type=&quot;metaChainExpressionSpecification&quot;>
            &lt;chain xsi:type=&quot;umlProperty&quot; metaclass=&quot;Transition&quot; property=&quot;trigger&quot;/>
            &lt;chain xsi:type=&quot;umlProperty&quot; metaclass=&quot;Trigger&quot; property=&quot;event&quot;/>
            &lt;chain xsi:type=&quot;umlProperty&quot; metaclass=&quot;SignalEvent&quot; property=&quot;signal&quot;/>
            &lt;chain xsi:type=&quot;umlProperty&quot; metaclass=&quot;Signal&quot; property=&quot;APPLIED_STEREOTYPES&quot;/>
            &lt;chain xsi:type=&quot;umlProperty&quot; metaclass=&quot;Stereotype&quot; property=&quot;name&quot;/>
        &lt;/expression>
    &lt;/argument>
    &lt;expression xsi:type=&quot;makeInlineExpressionSpecification&quot; language=&quot;Jython&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;>
        &lt;parameter name=&quot;arg1&quot; type=&quot;java.lang.Object&quot; ordered=&quot;false&quot; unique=&quot;false&quot; multiple=&quot;false&quot;/>
        &lt;parameter name=&quot;stereotypes&quot; type=&quot;java.lang.Object&quot; ordered=&quot;false&quot; unique=&quot;false&quot; multiple=&quot;false&quot;/>
        &lt;body>#
# This script gets a list of stereotypes and, if they are Signals
# it tries to build the Scope assuming they are owned by 
# a hierachy of &amp;lt;&amp;lt;Interface Block&amp;gt;&amp;gt;c
#
#
# String to hold the result
#
res = &quot;&quot;

#               
# Gets iterator to parameters.
#                                               

