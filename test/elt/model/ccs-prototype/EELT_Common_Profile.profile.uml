                        <subvertex xmi:type="uml:Pseudostate" xmi:id="39e11288-45b8-4e73-9488-54907d9e6fd2" name="" visibility="public"/>
                        <subvertex xmi:type="uml:State" xmi:id="961e39be-5cd9-45b8-8549-c6c80dd16405" name="NotReady" visibility="public">
                          <ownedComment xmi:id="6cd61d88-fc4d-446d-a8ee-bffb62983963" annotatedElement="961e39be-5cd9-45b8-8549-c6c80dd16405" body="&lt;html>&#xA;  &lt;head>&#xA;&#x9;&#x9;&lt;style>&#xA;&#x9;&#x9;&#x9;p {padding:0px; margin:0px;}&#xA;&#x9;&#x9;&lt;/style>&#xA;&#x9;&lt;/head>&#xA;  &lt;body>&#xA;    &lt;p>&#xA;The function is partially powered, powering or otherwise not initialized or initializing.&#xA;&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;No command requiring motion of mechanical parts is allowed, a part fom Init.&#xA;&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;&amp;#160;&#xA;&#xA;    &lt;/p>&#xA;&#xA;&lt;/body>&#xA;&lt;/html>"/>
                        </subvertex>
                        <subvertex xmi:type="uml:State" xmi:id="ce5b6874-ea87-4c20-9abd-208fe86c6f2e" name="Error" visibility="public">
                          <ownedComment xmi:id="a2314c35-6cb4-42d3-b1a8-2740e264e666" annotatedElement="ce5b6874-ea87-4c20-9abd-208fe86c6f2e" body="&lt;html>&#xA;  &lt;head>&#xA;&#x9;&#x9;&lt;style>&#xA;&#x9;&#x9;&#x9;p {padding:0px; margin:0px;}&#xA;&#x9;&#x9;&lt;/style>&#xA;&#x9;&lt;/head>&#xA;  &lt;body>&#xA;    &lt;p>&#xA;The function is partially powered, powering or otherwise not Operational, with indications of an error condition.&#xA;&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;The function enters automatically in this state in case of failures while in NotOperational.&#xA;&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;Failures that can trigger a transition to this State are dependent of the specific Function and shall be specified at design time (and based on the ICDs)&#xA;&#xA;    &lt;/p>&#xA;&#xA;&lt;/body>&#xA;&lt;/html>"/>
                        </subvertex>
                        <subvertex xmi:type="uml:State" xmi:id="bf9fadf7-f5b0-49ed-92de-000ba3c3fba8" name="Initializing" visibility="public">
                          <ownedComment xmi:id="f3511b36-fe16-484d-9226-354af7c107fb" annotatedElement="bf9fadf7-f5b0-49ed-92de-000ba3c3fba8" body="&lt;html>&#xA;  &lt;head>&#xA;&#x9;&#x9;&lt;style>&#xA;&#x9;&#x9;&#x9;p {padding:0px; margin:0px;}&#xA;&#x9;&#x9;&lt;/style>&#xA;&#x9;&lt;/head>&#xA;  &lt;body>&#xA;    &lt;p>&#xA;This is a &amp;lt;&amp;lt;transientState&amp;gt;&amp;gt;.&#xA;&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;The function is performing an initialization procedure.&#xA;&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;It will automatically move to Ready when the initialization will be successfully completed.&#xA;&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;It will fall back to NotReady in case of initialization failure.&#xA;&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;In this model the state has:&#xA;&#xA;    &lt;/p>&#xA;&#xA;    &lt;ol>&#xA;      &lt;li>&#xA;entry action:&amp;#160;StartInit        &#xA;&lt;br>Executed when entering the state, to perform a first set of initialization tasks&#xA;&#xA;      &lt;/li>&#xA;      &lt;li>&#xA;do action: DoInit        &#xA;&lt;br>To perform the bulk of the initialization tasks.        &#xA;&lt;br>When the do action is completed, the &amp;lt;&amp;lt;transientState&amp;gt;&amp;gt; exits automatically and, in case of success, transitions to Ready (an alternative would be to let an estimator taking care of evaluating the state and generate an event to trigger the transition).&#xA;&#xA;      &lt;/li>&#xA;    &lt;/ol>&#xA;&lt;/body>&#xA;&lt;/html>"/>
                          <doActivity xmi:type="uml:Activity" xmi:id="881ef142-2063-41e3-8db9-df85f85c665a" name="DoInit" visibility="public" isReentrant="true" node="ddcc6f3c-62c1-4a60-bdc5-b03e3044974c b238fd6b-7a6f-4904-8831-2d54b6bc3f03 d2d65363-3b0e-4f1b-983a-766ae2da2c89 d2a525e2-f942-4235-8cff-fb2fbd5dbf45">
