                  <ownedComment xmi:id="303b6646-ef71-4f59-b1bd-825f8fd5a751" annotatedElement="d81db71a-c716-49c2-9447-8b514086e45e" body="&lt;html>&#xA;  &lt;head>&#xA;&#x9;&#x9;&lt;style>&#xA;&#x9;&#x9;&#x9;p {padding:0px; margin:0px;}&#xA;&#x9;&#x9;&lt;/style>&#xA;&#x9;&lt;/head>&#xA;  &lt;body>&#xA;    &lt;p>&#xA;Device is switched off or not available (missing communication).&#xA;&#xA;    &lt;/p>&#xA;&#xA;    &lt;p>&#xA;This State shall be reflected in the Online Database and might be (or probably should be) the result of a quality estimation to BAD Quality.&#xA;&#xA;    &lt;/p>&#xA;&#xA;&lt;/body>&#xA;&lt;/html>"/>
                </subvertex>
                <subvertex xmi:type="uml:State" xmi:id="1133c110-3eec-4928-9731-ee364207ef8a" name="On" visibility="public">
                  <ownedComment xmi:id="3be53009-3369-43f8-9b38-7c3729f17e04" annotatedElement="1133c110-3eec-4928-9731-ee364207ef8a" body="&lt;html>&#xA;  &lt;head>&#xA;&#x9;&#x9;&lt;style>&#xA;&#x9;&#x9;&#x9;p {padding:0px; margin:0px;}&#xA;&#x9;&#x9;&lt;/style>&#xA;&#x9;&lt;/head>&#xA;  &lt;body>&#xA;    &lt;p>&#xA;&lt;i>Device is running.&lt;/i>&#xA;&#xA;    &lt;/p>&#xA;&#xA;&lt;/body>&#xA;&lt;/html>"/>
                  <region xmi:id="2d20eb3a-719a-4a5c-94f6-57bd0202eec6" name="" visibility="public">
                    <ownedComment xmi:id="fd430676-8bf3-43dc-af91-eda63013ab8d" annotatedElement="2d20eb3a-719a-4a5c-94f6-57bd0202eec6" body="This is the main region for the state machine."/>
                    <transition xmi:id="ceb6ab9a-0ff6-46c9-89f5-a24050901001" name="" visibility="public" kind="internal" source="1133c110-3eec-4928-9731-ee364207ef8a" target="1133c110-3eec-4928-9731-ee364207ef8a">
