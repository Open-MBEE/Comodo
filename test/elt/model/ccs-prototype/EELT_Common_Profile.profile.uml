            <body>##########################################################################
# G.Chiozzi - 20171103, 20180430, 201805 31
#        
# SetLinkToDoorsObject
#
# Note: old version used for Use Case description as word files in One Drive.
#
# This macro is used to parse a requirement imported from Doors and, 
# if it is a Use Case Requirement, 
#   - checks if it already contains a Use Case element,
#   - eventually create it          
#   - check the Use Case element has the correct name, otherwise fixes it
#   - sets the hyperlink of the Use Case to the html hiperlink 
#     (i.e. the One Drive page)
#
# For a Use Case Requirement, the _ExternalURL tag is assumed to contain a URL in the form:
#    https://europeansouthernobservatory-my.sharepoint.com
# as the hyperlink to the One Drive page. 
#
# Check the comments in the code for more details.
#
##########################################################################

from com.nomagic.magicdraw.core       import Application
from com.nomagic.magicdraw.core       import Project
from com.nomagic.uml2.ext.jmi.helpers import StereotypesHelper
from com.nomagic.requirements.reqif   import ReqIFConstants
from com.nomagic.magicdraw.sysml.util import SysMLProfile

from com.nomagic.magicdraw.automaton import AutomatonMacroAPI
from com.nomagic.uml2.impl import ElementsFactory
from com.nomagic.uml2.ext.magicdraw.mdusecases import UseCase
from com.nomagic.magicdraw.openapi.uml import ModelElementsManager
from com.nomagic.magicdraw.uml.symbols.shapes.html import HTMLHelper

import re

#
# Checks if the given model element is the owner of Use Cases
# Returns:
#     True if it is
#     False if not
#
def checkIfOwnsUseCase(element):
   #Application.getInstance().getGUILog().log(&quot;Selected element is: &quot; + element.getName())
   try: 
     if element != None:   
         useCaseList = element.getOwnedUseCase(); 
         if useCaseList:
            return True
         else:
            return False    
   except Exception, ex:
       Application.getInstance().getGUILog().log(&quot;Exception!: &quot; + str(ex))
   return False
    
# 
# Ensures that the given model element owns a use case with the same name of the model element.
# If the model element owns already a use case, it just returns
# Otherwise, it creates a new use case with the same name of the model element
# and adds it to the model element.
# If the model element cannot be a use case owner, and exception is risen (as well as if other things
# go wrong)
#  
def ensureRequirementOwnsUseCase(element):
    try:
        if checkIfOwnsUseCase(element) == True:
            # Gets the use cases     
            useCaseList = element.getOwnedUseCase();
            # Iterates over the use cases
            counter = 0 
            for useCase in useCaseList:
               #Application.getInstance().getGUILog().log(element.getName() + &quot; already owns Use Case &quot; + useCase.getName())
               counter = counter + 1
               if useCase.getName() == element.getName():
                  # There is one use case in the list with the right name. Finished. 
                  checkAndSetRequirementUseCaseStereotype(useCase)
                  return
            # If we are here, there are use case, but none with the right name
            if counter == 1:
               # If there is just one use case, we must rename it. Must be the right one!
               Application.getInstance().getGUILog().log(element.getName() + &quot; owns Use Case: &quot; + useCase.getName() + &quot; and I rename it&quot;)
               useCase.setName(element.getName())           
               checkAndSetRequirementUseCaseStereotype(useCase)
            else:
               # There are more use cases and I cannot know the one to rename.
               # log message
               Application.getInstance().getGUILog().log(element.getName() + &quot; owns Use Cases and I cannot decide which one to rename. Do it yourself&quot;)
            return                        
        else:
            Application.getInstance().getGUILog().log(element.getName() + &quot; does not own a Use Case. I will insert one.&quot;)
    
            # Create a Use Case
            factory = Application.getInstance().getProject().getElementsFactory()
            newUseCase = factory.createUseCaseInstance()
            newUseCase.setName(element.getName())                               
            checkAndSetRequirementUseCaseStereotype(newUseCase)
            # Here the opaque object is not sufficient and I need 
            # to retrieve from it the real model element
            ModelElementsManager.getInstance().addElement(newUseCase, element) 
    except Exception, ex:
        Application.getInstance().getGUILog().log(&quot;Exception!: &quot; + str(ex))


#
# Lists all the hyperlinks of a given Opaque Object
#
def listHypelinks(element):
    #
    # Get the &quot;HyperlinkOwner&quot; stereotype
    #
    project = Application.getInstance().getProject()
    hyperlinkOwnerStereotype = StereotypesHelper.getStereotype(project, &quot;HyperlinkOwner&quot;);
 
    # Get the list of all hyperlinks
    hyperlinks = StereotypesHelper.getStereotypePropertyValue(element, hyperlinkOwnerStereotype, &quot;hyperlinkText&quot;)
    # Print the value of each hypelink iterating over the list
    if hyperlinks:
        Application.getInstance().getGUILog().log(&quot;Existing hyperlinks for &quot; + element.getName())
    for hyperlink in hyperlinks:
        Application.getInstance().getGUILog().log(&quot;- hyperlink: &quot; + hyperlink)
    
#
# Checks if the given element already has the &lt;&lt;requirementUseCase>> stereotype and adds it if not
#
def checkAndSetRequirementUseCaseStereotype(element):
       
    #
    # Get the &quot;requirementUseCase&quot; stereotype
    #
    project = Application.getInstance().getProject()
    requirementUseCaseStereotype = StereotypesHelper.getStereotype(project, &quot;requirementUseCase&quot;);
           
    stereotypes = StereotypesHelper.getStereotypes(element)      
    #Application.getInstance().getGUILog().log(&quot;Existing stereotypes:&quot; + str(stereotypes))
    if not [x for x in stereotypes if x.getName() == &quot;requirementUseCase&quot;]:
       #Application.getInstance().getGUILog().log(&quot;Not requirementUseCase. Will add stereotype.&quot;)
       StereotypesHelper.addStereotype(element, requirementUseCaseStereotype);
       #StereotypesHelper.createDefaultValues(element, requirementUseCaseStereotype, True);

#
# Set the ActiveHyperlink of the given Opaque Element to the given link string (url)
# if not already done.
# It also cleans up Hyperlinks &quot;of the same type&quot;, i.e. containing the toBeCleaned
# string, but different from the current value.
# It is assumed that they are older URLs that have been updated.
#
def setHyperlink(element, link, toBeCleaned):

    #
    # Get the &quot;HyperlinkOwner&quot; stereotype
    #
    project = Application.getInstance().getProject()
    hyperlinkOwnerStereotype = StereotypesHelper.getStereotype(project, &quot;HyperlinkOwner&quot;);
    
    stereotypes = StereotypesHelper.getStereotypes(element)      
    #Application.getInstance().getGUILog().log(&quot;Existing stereotypes:&quot; + str(stereotypes))
    if not [x for x in stereotypes if x.getName() == &quot;HyperlinkOwner&quot;]:
        #Application.getInstance().getGUILog().log(&quot;Not Hyperlink Owner. Will add stereotype.&quot;)
        StereotypesHelper.addStereotype(element, hyperlinkOwnerStereotype);

    #
    # It looks like if I want to set the Active Hyperlink I have to set 
    # BOTH hyperlinkText and hyperlinkTextActive
    #
    
    #
    # Check if this is already the Active Hyperlink, otherwise sets it
    #
    activeHyperlink = StereotypesHelper.getStereotypePropertyValue(element, hyperlinkOwnerStereotype, &quot;hyperlinkTextActive&quot;)
    if activeHyperlink == link :
        #Application.getInstance().getGUILog().log(&quot;Active Hyperlink already properly set&quot;)
        pass
    else:
        StereotypesHelper.setStereotypePropertyValue(element, hyperlinkOwnerStereotype, &quot;hyperlinkTextActive&quot;, link, False)           
        #Application.getInstance().getGUILog().log(&quot;Active Hyperlink set&quot;)
    
    #
    # Check now if is is already in the list of normal hyperlinks
    # If a toBeCleaned string is given, it also deletes any hyperlink
    # different from the new one that contains that as a substring.
    # This allows to cleanup and update old definitions.
    #
    # Get the list of all hyperlinks
    hyperlinks = StereotypesHelper.getStereotypePropertyValue(element, hyperlinkOwnerStereotype, &quot;hyperlinkText&quot;)
    
    # Cleanup old links, if different from new one
    if toBeCleaned != None: 
        for hyperlink in hyperlinks:
            if hyperlink != link and toBeCleaned in hyperlink:
                #Application.getInstance().getGUILog().log(&quot;Cleanup older Hyperlink&quot;)
                slot = StereotypesHelper.getSlot(element, hyperlinkOwnerStereotype, &quot;hyperlinkText&quot;, False, False)
                StereotypesHelper.removeSlotValue(slot, hyperlink)

    # Now sets the hyperlink, if necessary
    if [hyperlink for hyperlink in hyperlinks if link == hyperlink]:
        #Application.getInstance().getGUILog().log(&quot;Hyperlink already properly set&quot;)
        pass
    else:
        StereotypesHelper.setStereotypePropertyValue(element, hyperlinkOwnerStereotype, &quot;hyperlinkText&quot;, link, True)           
        #Application.getInstance().getGUILog().log(&quot;Hyperlink set&quot;)

#
# For the given Use Case Requirement,
# - retrieves the Text tag
# - verrifies it looks like a valid OneDrive URL
# - calls the function to set to it the Active Hyperlink for the Use Case
#
def updateLinkOfReqUC(element):
    
    project = Project.getProject(element);

    # I get the Profile by name. should be possible to get it from com.nomacic.requirements.reqif contants as the next one
    reqIfProfile     = StereotypesHelper.getProfile(project, &quot;ReqIF Profile&quot;)
    reqIfPropProfile = StereotypesHelper.getProfile(project, ReqIFConstants.PROPERTIES_PROFILE_NAME)

    if reqIfPropProfile != None :
      #
      # Get the URL imported with this Doors requirement
      #
      stereotype = StereotypesHelper.getStereotype(project, &quot;ObjectProperties&quot;, reqIfPropProfile)       
      requirementStereotype = SysMLProfile.getInstance(project).getRequirement()

      if stereotype != None :
         isUseCase = StereotypesHelper.getStereotypePropertyFirst(element, stereotype, &quot;_Is Use Case&quot;)
         
         # Application.getInstance().getGUILog().log(&quot;=== &quot; + element.getName() + &quot; - Is Use Case: &quot; + str(isUseCase))
         
         if isUseCase == True:
            ensureRequirementOwnsUseCase(element)
            
            # String used to identify a (probably) good link to a OneDrive URL
            urlIdentifier = &quot;https://europeansouthernobservatory-my.sharepoint.com&quot;
            
            # Retrieves the pure text from the Use Case Requirement, removing any HTML
            # formatting.
            # We assume that the text is just the hyperlynk to the OneDrive page.
            extUrlTextProperty = StereotypesHelper.getStereotypePropertyFirst(element, stereotype, 
                                                     &quot;_ExternalURL&quot;)
            extUrlText = HTMLHelper.getPureTextFromHTMLSource(extUrlTextProperty)
            #Application.getInstance().getGUILog().log(&quot;=== &quot; + element.getName() + &quot; - URL:&quot; + extUrlText)
            
            if not urlIdentifier in extUrlText:
               #Application.getInstance().getGUILog().log(&quot;=== &quot; + element.getName() + &quot; - Text does not contain an hyperlink&quot;)
               pass
            else:
               
               #Application.getInstance().getGUILog().log(&quot;=== &quot; + element.getName() + &quot; - Hyperlink is:\n&quot; + extUrlText + &quot;\n&quot;)
               useCaseList = element.getOwnedUseCase();
               for useCase in useCaseList:
                  #Application.getInstance().getGUILog().log(&quot;- Names:&quot; + sel.Name + &quot; - &quot; + useCase.Name + &quot;\n&quot;) 
                  #listHypelinks(useCase)
                  setHyperlink(useCase, extUrlText, urlIdentifier)#
# Main()
#
updateLinkOfReqUC(requirement)

#=====================
</body>
