    <packagedElement xmi:type="uml:Package" xmi:id="eabe35e8-c8f9-4612-839f-afce08ebda0e" name="Actors" visibility="public" URI="">
      <ownedComment xmi:id="cd866ec4-617c-45fd-b0f9-27933dbacf09" annotatedElement="eabe35e8-c8f9-4612-839f-afce08ebda0e" body="This package lists and describes the actors identified for the operation of the ELT.&#xA;They are primarily used un Use Cases and activity/sequence diagrams."/>
      <packagedElement xmi:type="uml:Actor" xmi:id="70d9e989-e3ac-4b5e-b022-63aeeddb78b9" name="Operator" visibility="public">
        <generalization xmi:id="2b5985cb-8044-4d1e-ad1e-424000cf6615" general="214d0097-c688-49dd-bd84-b6f3d884bb2c"/>
      </packagedElement>
      <packagedElement xmi:type="uml:Actor" xmi:id="9e11291b-80fb-47b5-8cea-dfc1f1d7d47f" name="ELTManager" visibility="public">
        <ownedComment xmi:id="38c587af-63e5-4472-9d1b-c33bc088fef4" annotatedElement="9e11291b-80fb-47b5-8cea-dfc1f1d7d47f" body="Responsible for the telescope during the day. The ELTManager prepares the telescope for night operation and hands it over&#xA;to the Operator who is responsible for the telescope at night time for operation.&#xA;He is on-call at night time in case of problems."/>
        <generalization xmi:id="0fc4b8a1-75c7-499a-8032-e503d5b7d574" general="214d0097-c688-49dd-bd84-b6f3d884bb2c"/>
      </packagedElement>
      <packagedElement xmi:type="uml:Actor" xmi:id="214d0097-c688-49dd-bd84-b6f3d884bb2c" name="ESOTechnician" visibility="public">
        <ownedComment xmi:id="c5ba1a96-2e6f-46ac-8dca-e50068a0957d" annotatedElement="214d0097-c688-49dd-bd84-b6f3d884bb2c" body="Any ESO technician authorized to interact with the ELT telescope, the instrumentation and the support infrastructure.&#xA;This is a &quot;base class&quot; for more specialized actors, like telescope operators, engineers, maintenance personnel and so on. "/>
      </packagedElement>
      <packagedElement xmi:type="uml:Actor" xmi:id="474780d4-b1ec-45ae-8624-9bf024666d5c" name="Engineer" visibility="public">
        <ownedComment xmi:id="613d6653-a70f-4d2e-9119-48fc3bdcfdbd" annotatedElement="474780d4-b1ec-45ae-8624-9bf024666d5c" body="This actor is for the time being representing all Engineers vorking on the development, integration and testing of the Telescope.&#xA;It might be actually better to identify more detailed cathegories.&#xA;For example: SW Engineer, Testing Engineer...."/>
        <generalization xmi:id="1f3a31ce-e9c6-4c88-91c4-43efa3f1587b" general="214d0097-c688-49dd-bd84-b6f3d884bb2c"/>
      </packagedElement>
      <packagedElement xmi:type="uml:Actor" xmi:id="59547c26-8e51-412c-887a-0398dae35c06" name="Maintenance technician" visibility="public">
        <ownedComment xmi:id="0b9ca706-afbd-467a-91b0-d436c2af053e" annotatedElement="59547c26-8e51-412c-887a-0398dae35c06" body="Any technician (i.e. not just SW technicians) working on the maintenance, calibration and testing of a component of the Telescope."/>
        <generalization xmi:id="b8f615b0-c663-4da7-b954-9f4af59bf429" general="214d0097-c688-49dd-bd84-b6f3d884bb2c"/>
      </packagedElement>
    </packagedElement>
